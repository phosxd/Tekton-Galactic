config_version=5

[application]

config/name="Tekton Galactic"
config/version="indev"
run/main_scene="uid://bolad3oot6dtn"
run/print_header=false
config/use_hidden_project_data_directory=false
config/features=PackedStringArray("4.4", "GL Compatibility")
run/max_fps=120
boot_splash/bg_color=Color(0.0923971, 0.0923971, 0.092397, 1)
boot_splash/show_image=false
config/icon="res://CORE/icon.svg"

[autoload]

Init="*res://CORE/Scripts/Init.gd"
SandboxManager="*res://CORE/Scripts/SandboxManager.gd"

[debug]

file_logging/enable_file_logging.pc=false
gdscript/warnings/shadowed_variable=0
gdscript/warnings/shadowed_variable_base_class=0
gdscript/warnings/shadowed_global_identifier=0

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/Blueprint/plugin.cfg")

